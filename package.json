{
  "name": "@codewithcheese/vite-plugin-obsidian",
  "version": "0.1.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./webview": {
      "require": {
        "types": "./dist/webview.d.ts",
        "default": "./dist/webview.js"
      },
      "import": {
        "types": "./dist/webview.d.mts",
        "default": "./dist/webview.mjs"
      }
    },
    "./client": "./dist/client.global.js",
    "./env": "./env.d.ts"
  },
  "packageManager": "pnpm@9.1.4",
  "scripts": {
    "dev": "pnpm clean && tsup --watch",
    "build": "pnpm clean && tsup",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "builtin-modules": "3.3.0",
    "dayjs": "^1.11.13",
    "esbuild": "0.17.3",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-svelte": "^0.9.0",
    "kolorist": "^1.8.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0"
  },
  "devDependencies": {
    "@tomjs/tsconfig": "^1.7.1",
    "@types/node": "^22.13.4",
    "globals": "^15.13.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.0"
  },
  "peerDependencies": {
    "vite": ">=6"
  }
}
